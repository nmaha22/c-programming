#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char name[50];
    int m1,m2,m3,m4,m5,total;
    float avg;
    scanf("%s",name);
    printf("Name of the Student:%s\n",name);
    scanf("%d",&m1);
    scanf("%d",&m2);
    scanf("%d",&m3);
    scanf("%d",&m4);
    scanf("%d",&m5);
    total=m1+m2+m3+m4+m5;
    printf("Total Mark:%d\n",total);
    avg=total/5.0;
    printf("Average Mark:%.1f\n",avg);
    if(avg==100)
    {
        printf("Grade Mark:s");
    }
    else if(avg>=90)
    {
        printf("Grade Mark:A");
    }
    else if(avg>=80)
    {
        printf("Grade Mark:B");
    }
    else if(avg>=70)
    {
        printf("Grade Mark:C");
    }
    else if(avg>=60)
    {
        printf("Grade Mark:D");
    }
    else if(avg>=50)
    {
        printf("Grade Mark:E");
    }
    else 
    {
        printf("Grade Mark:Fail");
    }   
    return 0;
}
