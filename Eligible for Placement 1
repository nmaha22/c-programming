#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char name[50];
    int arrear,cpga;
    scanf("%s",name);
    scanf("%d",&arrear);
    scanf("%d",&cpga);
    printf("Name of the Student:%s\n",name);
    if((arrear == 1 && cpga > 70)||(arrear <= 2 && cpga > 75))
       {
           printf("%s is Eligible for Placement",name);
       }
       else
       {
           printf("%s is Not Eligible for Placement",name);
       }
    return 0;
}
